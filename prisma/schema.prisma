// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Register {
  id          String @id @default(uuid())
  description String
  type        String
  date        String
  value       String
  status      String
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String
  photo    String
  password String
}
